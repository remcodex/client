{
    "name": "remcodex/client",
    "description": "Remote code execution client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Remcodex",
            "email": "remotecodexecution@outlook.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "ahmard/guzwrap": "^2.3",
        "opis/closure": "^3.6",
        "nette/utils": "^3.2",
        "evenement/evenement": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.64",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Remcodex\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Remcodex\\Client\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyse": "vendor/bin/phpstan analyse"
    }
}
